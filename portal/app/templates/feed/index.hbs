<div style="margin-top: 14px">
  <h6>Top 7 hooks</h6>

  {{#each item in model}}
    <div class="media" {{action 'navToDetail'}}>
      <div class="media-left">
        <a href="#">
          <img class="media-object" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;">
        </a>
      </div>
      <div class="media-body">
        <h4 class="media-heading" id="media-heading">
          <a class="anchorjs-link" href="#media-heading">
            <span class="anchorjs-icon">Tư Cuồng</span>
          </a>
        </h4>
        {{!-- <i class="glyphicon glyphicon-earphone"></i>{{item.phoneNumber}}<br/> --}}
        {{item.content}}
      </div>
    </div>
  {{/each}}

</div>
